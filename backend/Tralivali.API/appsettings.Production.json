{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDb": {
    "ConnectionString": "#{MongoDb_ConnectionString}#",
    "DatabaseName": "tralivali"
  },
  "RabbitMq": {
    "HostName": "#{RabbitMq_HostName}#",
    "Port": 5672,
    "UserName": "#{RabbitMq_UserName}#",
    "Password": "#{RabbitMq_Password}#",
    "VirtualHost": "/"
  },
  "Jwt": {
    "SecretKey": "#{Jwt_SecretKey}#",
    "Issuer": "tralivali",
    "Audience": "tralivali-users",
    "ExpirationMinutes": 1440
  },
  "AzureCommunication": {
    "ConnectionString": "#{AzureCommunication_ConnectionString}#",
    "SenderEmail": "noreply@tralivali.com"
  }
}
